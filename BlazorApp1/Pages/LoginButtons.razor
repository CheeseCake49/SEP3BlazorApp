@using BlazorApp1.Services.Http
@namespace Login
@inject NavigationManager navMgr
@inject IAuthService authService

<AuthorizeView>
            <Authorized>
                <MudIconButton Icon="@Icons.Material.Filled.Logout" Title="Logout" OnClick="Logout"></MudIconButton>
            </Authorized>
            <NotAuthorized>
                <MudIconButton Icon="@Icons.Material.Filled.Login" Title="Login" OnClick="Login"></MudIconButton>
            </NotAuthorized>
        </AuthorizeView>

@code {

    private void Login()
    {
        navMgr.NavigateTo("/Login");
    }

    private async Task Logout()
    {
        await authService.LogoutAsync();
        navMgr.NavigateTo("/");
    }

}

